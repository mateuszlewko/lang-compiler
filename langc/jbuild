(jbuild_version 1)

(executable
 ((name langc)
  (libraries (core batteries lang_compiler llvm.analysis))))

(library
 ((name    langc_lib)
  (wrapped true)
  (libraries (core batteries lang_compiler llvm.analysis))
  (preprocess (pps (ppx_deriving.std ppx_enumerate)))))

(alias
  ((name runlangc)
   (deps ((file langc.exe)))))